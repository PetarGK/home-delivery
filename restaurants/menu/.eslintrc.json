{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    },
    "env": {
        "jest": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    }
}